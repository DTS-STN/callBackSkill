{
    "name": "callback-skill",
    "version": "1.0.0",
    "description": "Learning skills development",
    "author": "",
    "license": "MIT",
    "main": "lib/index.js",
    "scripts": {
        "clean": "rimraf ./lib",
        "copy-templates": "copyfiles --up 1 \"./src/**/*.{json,lg}\" \"./lib/src\"",
        "prebuild": "npm run lint",
        "build": "tsc --build && npm run copy-templates",
        "lint": "eslint ./src/**/*.ts",
        "lint-fix": "eslint --fix ./src/**/*.ts",
        "start": "npm run build && nodemon --exec ts-node src/app.ts",
        "watch": "nodemon ./lib/index.js NODE_ENV=development",
        "test": "mocha",
        "coverage": "nyc mocha",
        "test-coverage-ci": "nyc --reporter=cobertura mocha --reporter mocha-junit-reporter"
    },
    "dependencies": {
        "bot-solutions": "^1.1.0",
        "botbuilder": "^4.13.5",
        "botbuilder-ai": "^4.15.0",
        "botbuilder-testing": "~4.14.1",
        "botbuilder-applicationinsights": "^4.13.5",
        "botbuilder-azure": "^4.13.5",
        "botbuilder-azure-blobs": "^4.13.5-preview",
        "botbuilder-dialogs": "^4.13.5",
        "botbuilder-lg": "^4.13.5",
        "botframework-config": "^4.13.5-deprecated",
        "botframework-connector": "^4.13.5",
        "dotenv": "^6.0.0",
        "i18n": "^0.13.3",
        "path": "^0.12.7",
        "restify": "^8.5.1",
        "ts-node": "~10.2.1",
        "ts-sinon": "^2.0.2"
    },
    "devDependencies": {
        "@types/chai": "~4.2.22",
        "@types/dotenv": "~8.2.0",
        "@types/mocha": "~9.0.0",
        "@types/node": "^10.17.27",
        "@types/restify": "~8.5.2",
        "@typescript-eslint/eslint-plugin": "^2.0.0",
        "@typescript-eslint/parser": "^2.0.0",
        "copyfiles": "^2.1.0",
        "eslint": "^5.16.0",
        "eslint-plugin-only-warn": "^1.0.1",
        "mocha": "^6.1.4",
        "mocha-junit-reporter": "^1.22.0",
        "nock": "^10.0.6",
        "nodemon": "^2.0.2",
        "nyc": "^14.1.1",
        "replace": "^1.0.0",
        "rimraf": "^2.6.2",
        "typescript": "^3.2.2"
    },
    "env": {
        "mocha": true,
        "node": true
    }
}
